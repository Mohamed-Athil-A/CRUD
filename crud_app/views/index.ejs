<!-- Main Site -->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/add-user" class="box-shadow">
        <span class="text-gradiant"
          >New User <i class="fa-solid fa-user"></i
        ></span>
      </a>
    </div>
    <!-- Form handling -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>@Email</th>
            <th>Gender</th>
            <th>status</th>
            <th>action</th>
          </tr>
        </thead>
        <tbody>
          <%-include('include/_show')%>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- /Main Site -->
